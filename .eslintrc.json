{
  "extends": ["next/core-web-vitals", "prettier"],
  "plugins": ["react"],
  "rules": {
    "react/jsx-sort-props": [
      "error",
      {
        "callbacksLast": true,
        "shorthandFirst": true,
        "shorthandLast": false,
        "ignoreCase": true,
        "noSortAlphabetically": false,
        "reservedFirst": true,
        "multiline": "last"
      }
    ]
  }
}
